{
  "extends": [
    "tslint:recommended",
    "tslint-react"
  ],
  "rules": {
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "curly": true,
    "class-name": true,
    "object-literal-sort-keys": false,
    "max-classes-per-file": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "interface-name": [
      false
    ],
    "member-access": [
      false
    ],
    "semicolon": [
      true,
      "always"
    ],
    "triple-equals": true,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "arrow-parens": [
      true,
      "ban-single-arg-parens"
    ],
    "jsx-no-multiline-js": false
  }
}
