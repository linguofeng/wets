{
  "extends": ["tslint:latest", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": [
      true,
      {
        "singleQuote": true,
        "trailingComma": "all"
      }
    ],
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-eval": false
  }
}
