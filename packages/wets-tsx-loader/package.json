{
  "name": "wets-tsx-loader",
  "version": "1.0.0",
  "description": "wets tsx loader for webpack",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/linguofeng/wets.git",
    "directory": "packages/wets-tsx-loader"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clear-debug": "rimraf debug",
    "pretest": "yarn clear-debug",
    "test": "jest",
    "test:dev": "NODE_ENV=dev jest",
    "build": "gulp build",
    "build-w": "gulp build:watch",
    "clean": "rimraf dist",
    "prepub": "yarn clear-debug && yarn build",
    "pub": "mnpm publish"
  },
  "author": "lixucheng",
  "license": "ISC",
  "dependencies": {
    "acorn-jsx": "^4.0.1",
    "acorn-jsx-walk": "^1.0.1",
    "deepcopy": "^0.6.3",
    "js-beautify": "^1.6.14",
    "loader-utils": "^1.1.0",
    "ts-loader": "^2.3.7"
  },
  "devDependencies": {
    "@types/acorn": "^4.0.2",
    "@types/jest": "^20.0.8",
    "@types/node": "^8.0.20",
    "@types/ramda": "^0.24.7",
    "babel-eslint": "^7.2.3",
    "eslint": "^4.4.1",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "gulp": "^3.9.1",
    "gulp-typescript": "^3.2.1",
    "jest": "^21.0.2",
    "jest-html-reporter": "^0.5.3",
    "ramda": "^0.24.1",
    "rimraf": "^2.6.1",
    "ts-jest": "^21.0.0",
    "tslint": "^5.6.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-plugin-prettier": "^1.3.0"
  },
  "jest": {
    "testResultsProcessor": "./node_modules/jest-html-reporter",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "jest-html-reporter": {
    "pageTitle": "wets-tsx-loader",
    "outputPath": "test/index.html",
    "includeFailureMsg": true
  }
}